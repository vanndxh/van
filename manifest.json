{
  "name": "van",
  "description": "写着玩的小插件",
  "version": "0.0.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "./index.html",
    "default_icon": "./logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/index.js"],
      "run_at": "document_start"
    }
  ]
}
